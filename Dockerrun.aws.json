{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "atsea",
      "image": "rkoron/atsea",
      "environment": [
        {
          "name": "DATABASE_URL",
          "value": "postgres://postgres@db"
        }
      ],
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "links": [
        "db"
      ]
    },
    {
      "name": "db",
      "image": "postgres:10-alpine",
    "mountPoints": [
      {
        "containerPath": "/var/lib/postgresql/data"
      }
      ]
    }
  ]
}